<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <TargetFrameworks>net10.0;net9.0</TargetFrameworks>
        <PackageLicenseExpression>LGPL-3.0+</PackageLicenseExpression>
        <EnableTrimAnalyzer>true</EnableTrimAnalyzer>
        <EnableAoTAnalyzer>true</EnableAoTAnalyzer>
        <IsTrimmable>true</IsTrimmable>
        <IsAoTCompatible>true</IsAoTCompatible>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="EnhancedLinq" Version="1.0.0-beta.1" />
      <PackageReference Include="Microsoft.Extensions.Http" Version="10.0.3" />
      <PackageReference Include="NuGet.Protocol" Version="7.3.0" />
    </ItemGroup>

    <ItemGroup Condition="'$(TargetFramework)' == 'net9.0'">
      <PackageReference Include="System.Linq.AsyncEnumerable" Version="10.0.3" />
    </ItemGroup>
    
</Project>